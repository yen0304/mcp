{
  "version": "0.2.0",
  "configurations": [
  {
      "type": "node",
      "request": "launch",
      "name": "Client.ts",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ts-node-dev",
      "args": [
      "--respawn",
      "--transpile-only",
      ],
      "skipFiles": [
      "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/index.ts",
      "console": "integratedTerminal",
    }
  ]
}